{
  "hosting": {
    "public": "./dist/apps/admin",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/api/:path*",
        "destination": "https://api.testadmindomain.xyz/api/:path*",
        "type": 301
      }
    ]      
  }
}
